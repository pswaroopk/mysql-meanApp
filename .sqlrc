CREATE TABLE `user_profile` (
  `device_uuid` varchar(64) NOT NULL,
  `mobile_country_code` varchar(16) NOT NULL,
  `mobile_number` varchar(24) NOT NULL,
  `user_status` int(11) DEFAULT NULL,
  `user_id` varchar(64) NOT NULL,
  `create_dts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `termination_dts` timestamp NULL DEFAULT NULL,
  `active` tinyint(1) DEFAULT NULL,
  `lang` varchar(8) DEFAULT NULL,
  `last_update_dts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `email` varchar(64) DEFAULT NULL,
  `bts_customer_id` bigint(20) DEFAULT NULL,
  `device_id` bigint(20) DEFAULT NULL,
  `email_verified` tinyint(1) NOT NULL DEFAULT '0',
  FOREIGN KEY (user_id) REFERENCES user_names(user_id) ON UPDATE CASCADE ON DELETE RESTRICT,
  KEY `idx_phone_number` (`mobile_number`,`mobile_country_code`),
  KEY `idx_email` (`email`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1


CREATE TABLE `user_profile` (
